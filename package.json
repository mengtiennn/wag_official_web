{
  "name": "fa_da_tsai_system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "gen": "node ./genApi.js",
    "genSchemas": "node ./genApiSchemas.js",
    "gen_odata": "openapi-generator-cli generate -i apiSource/api.json -g typescript-axios -o ./src/api/dataApi --additional-properties=useSingleRequestParameter=true,withSeparateModelsAndApi=true,apiPackage=dataApi,modelPackage=dataModel -t ./my-templates",
    "gen_rtraffic": "openapi-generator-cli generate -i https://dev.maas.foxconn.com/rtraffic/maas/v1/swagger.json -g typescript-axios -o ./src/api/rtrafficApi --additional-properties=useSingleRequestParameter=true -t ./my-templates",
    "gen_rgoogle": "openapi-generator-cli generate -i https://qa.citygpt.foxconn.com/rgoogle/maas/v1/swagger.json -g typescript-axios -o ./src/api/rgoogleApi --additional-properties=useSingleRequestParameter=true -t ./my-templates",
    "gen_rbasic": "openapi-generator-cli generate -i http://52.185.181.171/rbasic/maas/v1/swagger.json -g typescript-axios -o ./src/api/rbasicApi --additional-properties=useSingleRequestParameter=true -t ./my-templates",
    "gen_rtdx": "openapi-generator-cli generate -i https://qa.citygpt.foxconn.com/rtdx/maas/v1/swagger.json -g typescript-axios -o ./src/api/rtdxApi --additional-properties=useSingleRequestParameter=true -t ./my-templates",
    "gen_bsuper": "openapi-generator-cli generate -i https://qa.citygpt.foxconn.com/bsuper/maas/v1/swagger.json -g typescript-axios -o ./src/api/bsuperApi --additional-properties=useSingleRequestParameter=true -t ./my-templates",
    "gen_rstorage": "openapi-generator-cli generate -i https://qa.citygpt.foxconn.com/rstorage/maas/v1/swagger.json -g typescript-axios -o ./src/api/rstorage --additional-properties=useSingleRequestParameter=true -t ./my-templates",
    "gen_ridp": "openapi-generator-cli generate -i https://dev.citygpt.foxconn.com/ridp/maas/v1/swagger.json -g typescript-axios -o ./src/api/ridp --additional-properties=useSingleRequestParameter=true -t ./my-templates",
    "gen_all": "npm run gen_rtraffic && npm run gen_rbasic && npm run gen_bsuper && npm run gen_rgoogle && npm run gen_rstorage --skip-validate-spec"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.1",
    "axios": "^1.6.7",
    "date-fns": "^4.1.0",
    "echarts": "^5.5.1",
    "html2canvas": "^1.4.1",
    "js-file-download": "^0.4.12",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^3.48.1",
    "ts-odata-client": "^2.0.2",
    "vue": "^3.4.29",
    "vue-easy-lightbox": "^1.19.0",
    "vue-echarts": "^7.0.3",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.2.5",
    "vue-scroll-picker": "2.0.0",
    "vue3-markdown": "^1.1.9"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.1",
    "terser": "^5.31.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.21"
  }
}
